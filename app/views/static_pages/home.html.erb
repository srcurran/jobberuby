<h1>Home</h1>
<% if logged_in? %>
  <div class="row">
    <div class="col-md-8">
      <section class="user_info">
        Hello, <%= render 'shared/current_user' %>
      </section>
    </div>
    <div class="col-md-8">
      <h3>Todo List</h3>
      <%= render 'shared/feed' %>
      <section class="todo_form">
        <%= render 'shared/todo_form' %>
      </section>
    </div>
  </div>



<% else %>
  <p>Find me in app/views/static_pages/home.html.erb</p>
  <p><%= link_to "Register", register_path %> </p>
  <p><%= link_to "Log in", login_path %> </p>
<% end %>
